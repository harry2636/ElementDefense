              wind_x = lengthdir_x(wind_proj_spd*5+power(spd, 3), dir);
              wind_y = lengthdir_y(wind_proj_spd*5+power(spd, 3), dir);
              physics_apply_impulse(phy_position_x, phy_position_y, wind_x, wind_y);
