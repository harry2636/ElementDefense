  timer = (timer + 1) % (70 - atk_spd);
}
if (timer == 0 &amp;&amp; instance_exists(obj_enemy_parent) &amp;&amp; type != 5)
{
    nearest_enemy = instance_nearest(phy_position_x, phy_position_y, obj_enemy_parent);
    dist_enemy = point_distance(x, y, nearest_enemy.phy_position_x, nearest_enemy.phy_position_y);
    if (type != 2 || lvl != 6){
        if (dist_enemy &lt; 150){
            bullet = instance_create(phy_position_x, phy_position_y, bullet_object);
            bullet.tower_x = phy_position_x;
            bullet.tower_y = phy_position_y;
            bullet.lvl = lvl;
            bullet.type = type;
            bullet.dmg = dmg;
            bullet.proj_spd = proj_spd;
            with(bullet){
              event_user(0);
            }
            bullet.target = nearest_enemy;
        }
    }
    else{
      if (dist_enemy &lt; 80){
          wind_effect = instance_create(x, y, obj_effect_wind);
          obj_enemy_parent.wind_effect = wind_effect;
          obj_enemy_parent.wind_splash_dmg = dmg;
